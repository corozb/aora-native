{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/index.tsx",
    "app.d.ts",
    "lib/useAppwrite.jsx",
    "components/VideoCard.jsx",
    "components/Trending.jsx",
    "app/(tabs)/home.jsx",
    "app/search/[query].jsx",
    "app/(tabs)/profile.tsx"
  ]
}
